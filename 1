option = webdriver.ChromeOptions()
option.add_argument(r"user-data-dir=C:\Users\weiwulun\AppData\Local\Google\Chrome\User Data1")
browser = webdriver.Chrome(options=option)

#browser = webdriver.Chrome()#定义浏览器为chrome
url = f'https://docs.qq.com/sheet/DWEFCTXdETEN6SWp5?tab=BB08J2'#定义访问网址     {内为之前定义的变量hero}
bb = u'18233015112'
